{
  "name": "@jvllmr/react-component-titles",
  "version": "1.0.0",
  "description": "Component-bound titles for React",
  "type": "module",
  "repository": {
    "url": "https://github.com/jvllmr/component-titles",
    "type": "git",
    "directory": "packages/react-component-titles"
  },
  "homepage": "https://jvllmr.github.io/component-titles",
  "author": "Jan Vollmer <jan@vllmr.dev>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test:build": "publint --strict",
    "test": "vitest run --coverage",
    "prepublish": "build"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@jvllmr/component-titles-core": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@jvllmr/component-titles-test": "workspace:*",
    "@testing-library/react": "^14.0.0",
    "@types/react": ">=16.0.0",
    "react": ">=16.0.0"
  },
  "keywords": [
    "document",
    "DOM",
    "title",
    "core",
    "component",
    "react"
  ]
}
